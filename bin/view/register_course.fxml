<VBox prefHeight="354.0" prefWidth="391.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="controllers.RegisterCourseController">
   <Pane prefHeight="74.0" prefWidth="391.0">
