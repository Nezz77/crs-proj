          <ComboBox fx:id="semesterComboBox" layoutX="14.0" layoutY="12.0" prefHeight="38.0" prefWidth="149.0" promptText="Select Semester" />
          <ComboBox fx:id="courseComboBox" layoutX="202.0" layoutY="12.0" prefHeight="38.0" prefWidth="149.0" promptText="Select Course" />
      </children>
   </Pane>
   <Pane prefHeight="157.0" prefWidth="391.0">
      <children>
          <Button layoutX="206.0" layoutY="73.0" onAction="#handleBack" prefHeight="31.0" prefWidth="70.0" text="Back" />
